# ai record template for register inputs
record(ai, "$(P)$(R)") {
    field(DTYP,"asynInt32")
    field(INP,"@asynMask($(PORT) $(OFFSET) $(BITS))MODBUS_DATA")
    field(LINR,"NO CONVERSION")
    field(AOFF,"$(AOFF)")
    field(ASLO,"$(ASLO)")
    field(PREC,"$(PREC)")
    field(SCAN,"$(SCAN)")
    field(EGU,"$(EGU)")
    field(DESC,"$(DESC)")
}

