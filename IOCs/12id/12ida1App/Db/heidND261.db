grecord(ai,"$(P)HeidND261_read")
{
	field(DESC,"$(DESC) (raw)")
	field(DTYP,"asyn ai HeidND261")
	field(FLNK,"$(P)HeidND261_ave.PROC PP MS")
	field(INP,"@asyn($(PORT))")
	field(PREC,"6")
	field(EGU,"degrees")
}

grecord(calc,"$(P)HeidND261_ave")
{
	field(DESC,"$(DESC) (avg)")
	field(CALC,"(a+b+c+d+e+f+g+h+i+j)/10")
	field(INPA,"$(P)HeidND261_read.VAL NPP NMS")
	field(INPB,"$(P)HeidND261_ave.LA NPP NMS")
	field(INPC,"$(P)HeidND261_ave.LB NPP NMS")
	field(INPD,"$(P)HeidND261_ave.LC NPP NMS")
	field(INPE,"$(P)HeidND261_ave.LD NPP NMS")
	field(INPF,"$(P)HeidND261_ave.LE NPP NMS")
	field(INPG,"$(P)HeidND261_ave.LF NPP NMS")
	field(INPH,"$(P)HeidND261_ave.LG NPP NMS")
	field(INPI,"$(P)HeidND261_ave.LH NPP NMS")
	field(INPJ,"$(P)HeidND261_ave.LI NPP NMS")
	field(EGU,"degrees")
	field(PREC,"5")
}
